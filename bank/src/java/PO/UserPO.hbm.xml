<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="PO.UserPO" table="user">
    <id name="id" type="int">
      <column name="id"/>
      <generator class="increment"/>
    </id>
    <property name="bId" type="string">
      <column length="30" name="bId" not-null="true"/>
    </property>
    <property name="cPass" type="string">
      <column length="30" name="cPass" not-null="true"/>
    </property>
    <property name="cId" type="string">
      <column length="30" name="cId" not-null="true"/>
    </property>
    <property name="phone" type="string">
      <column length="30" name="phone" not-null="true"/>
    </property>
    <property name="name" type="string">
      <column length="30" name="name" not-null="true"/>
    </property>
    <property name="money" type="double">
      <column length="30" name="money" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
