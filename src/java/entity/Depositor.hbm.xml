<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="entity.Depositor" table="depositor">
    <id name="id" type="int">
      <column name="id"/>
      <generator class="increment"/>
    </id>
    <property name="address" type="string">
      <column length="30" name="address" not-null="true"/>
    </property>
    <property name="id_number" type="string">
      <column length="30" name="id_number" not-null="true"/>
    </property>
    <property name="phone" type="string">
      <column length="30" name="phone" not-null="true"/>
    </property>
    <property name="name" type="string">
      <column length="30" name="name" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
